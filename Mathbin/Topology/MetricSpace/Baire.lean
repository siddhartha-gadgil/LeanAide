instance (priority := 100) baire_category_theorem_emetric_complete [PseudoEmetricSpace Î±] [CompleteSpace Î±] :
    BaireSpace Î± := by
  refine' âŸ¨fun f ho hd => _âŸ©
  let B : â„• â†’ â„â‰¥0âˆ := fun n => 1 / 2 ^ n
